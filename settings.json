{
  "diffEditor.renderSideBySide": false,
  "editor.cursorSmoothCaretAnimation": false,
  "editor.cursorStyle": "block",
  "editor.fontFamily": "'Ubuntu Mono','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "trailing",
  "editor.scrollBeyondLastLine": false,
  "files.insertFinalNewline": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "python.formatting.provider": "black",
  "python.insidersChannel": "off",
  "remote.SSH.lockfilesInTmp": true,
  "remote.SSH.remoteServerListenOnSocket": true,
  "remote.SSH.useLocalServer": false,
  "remote.containers.cacheVolume": false,
  "remote.containers.copyGitConfig": false,
  "remote.containers.gitCredentialHelperConfigLocation": "none",
  "remote.containers.logLevel": "trace",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontFamily": "Ubuntu Mono,PowerlineSymbols",
  "terminal.integrated.fontWeight": "normal",
  "terminal.integrated.fontWeightBold": "normal",
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.persistentSessionScrollback": 1000,
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.showLinkHover": false,
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/local/bin/nvim",
  "vscode-neovim.useCtrlKeysForNormalMode": false,
  "window.dialogStyle": "custom",
  "window.menuBarVisibility": "toggle",
  "window.newWindowDimensions": "maximized",
  "window.openFoldersInNewWindow": "off",
  "window.openWithoutArgumentsInNewWindow": "off",
  "window.title": "${remoteName}",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 1,
  "workbench.colorTheme": "Ayu Dark Bordered",
  "workbench.editor.enablePreview": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "newUntitledFile"
}
